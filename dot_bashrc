# -*- mode: sh -*-
# vim: filetype=sh

# Source run commands only for interactive shells
[[ $- != *i* ]] && return

# Plugin manager
command -v sheldon &>/dev/null && eval "$(sheldon source)" # Load plugin manager

# Source local run commands
rc_file=~/.bashrc.local
[[ -f $rc_file ]] && source "$rc_file"
unset rc_file
